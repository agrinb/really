
  <div class="panel panel-primary">
    <div class="panel-heading">
      <h3 class="panel-title"><%= @appointment.meeting %>
        <span class="pull-right">Zip Code: <%= @appointment.property.zip_code %></span>
      </h3>
    </div>
    <div class="panel-body">
      <div class="media col-md-3">
        <figure class="pull-left">
           <img class="media-object img-rounded img-responsive"  src=<%= @appointment.property.photo %>  >
        </figure>
      </div>
      <div class="class=col-md-6 col-sm-offset-3">
              <h4 class="list-group-item-heading"><%= @appointment.property.name %></h4>
              <p class="list-group-item-text"><%= @appointment.property.description %></p>
              <p class="list-group-item-text"><%= @appointment.instructions %></p>
              <p class="list-group-item-text">Bedrooms: <%= @appointment.property.bedrooms %></p>
              <p class="list-group-item-text">Bathrooms: <%= @appointment.property.bathrooms %></p>
              <p class="list-group-item-text">Details: <%= @appointment.property.details %></p>
              <p class="list-group-item-text">Vistor: <%= @appointment.visitor %></p>
              <p class="list-group-item-text">Vistor Phone: <%= @appointment.visitor_phone %></p>
              <p class="list-group-item-text">Vistor Email: <%= @appointment.visitor_email %></p>
      </div>
        <div>
        <%= link_to "Back", agent_appointments_path, class: "btn btn-warning" %>
        </div>
    </div>
  </div>
